<h1>Attempt Access</h1>
<%= form_for :login do |f| %>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email, autofocus: true %>
</div>
<div>
    <%= f.label :password %>
    <%= f.password_field :password %>
</div>
<div>
    <%= f.submit "Confirm" %>
</div>
<% end %>

